{
  "name": "sqlpad",
  "version": "2.4.2",
  "description": "Web app for writing and running SQL queries and visualizing the results. Supports Postgres, MySQL, SQL Server, Crate and Vertica.",
  "license": "MIT",
  "author": {
    "name": "Rick Bergfalk",
    "email": "rick.bergfalk@gmail.com"
  },
  "engines": {
    "node": ">=6"
  },
  "contributors": [
    {
      "name": "D. Can Celasun",
      "email": "dcelasun@gmail.com"
    }
  ],
  "keywords": [
    "sql",
    "mssql",
    "postgres",
    "postgresql",
    "mysql",
    "crate",
    "presto",
    "vertica"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rickbergfalk/sqlpad"
  },
  "bugs": {
    "url": "https://github.com/rickbergfalk/sqlpad/issues",
    "email": "rick.bergfalk@gmail.com"
  },
  "proxy": "http://localhost:3010",
  "scripts": {
    "build": "react-scripts build",
    "coverage": "istanbul cover _mocha -- --recursive --exit",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "lint": "eslint '**/*.js'",
    "start-client-dev": "react-scripts start",
    "start-server-dev": "node-dev server.js --dir ./db --port 3010 --debug",
    "start": "concurrently \"npm run start-server-dev\" \"npm run start-client-dev\"",
    "test-client": "react-scripts test --env=jsdom",
    "test": "npm run lint && mocha --recursive --exit"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "async": "^2.6.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "detect-port": "^1.2.2",
    "errorhandler": "^1.4.0",
    "express": "^4.16.2",
    "joi": "^12.0.0",
    "json2csv": "^3.11.5",
    "latest-version": "^3.1.0",
    "lodash": "^4.13.1",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.20.1",
    "morgan": "^1.9.0",
    "mssql": "^3.0.0",
    "mysql": "^2.15.0",
    "nedb": "^1.8.0",
    "node-crate": "^1.0.27",
    "node-fetch": "^1.7.2",
    "node-xlsx": "^0.11.2",
    "nodemailer": "^4.4.2",
    "passport": "^0.4.0",
    "passport-google-oauth2": "^0.1.6",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "pg": "^6.0.0",
    "pg-cursor": "^1.0.1",
    "request": "^2.58.0",
    "rimraf": "^2.5.4",
    "sanitize-filename": "^1.6.1",
    "semver-diff": "^2.1.0",
    "serve-favicon": "^2.4.5",
    "socksjs": "^0.5.0",
    "uuid": "^3.2.1",
    "vertica": "^0.5.5"
  },
  "main": "server.js",
  "bin": {
    "sqlpad": "./server.js"
  },
  "devDependencies": {
    "@rickbergfalk/sqlpad-react-scripts": "^1.0.13",
    "babel-eslint": "^7.2.3",
    "babel-runtime": "^6.26.0",
    "bootstrap": "^3.3.7",
    "brace": "^0.11.0",
    "concurrently": "^3.5.1",
    "d3": "^3.5.17",
    "deep-equal": "^1.0.1",
    "ejs": "^2.5.6",
    "eslint": "^4.16.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "fixed-data-table-2": "^0.8.9",
    "fs-extra": "^5.0.0",
    "husky": "^0.14.3",
    "istanbul": "^0.4.4",
    "keymaster": "^1.6.2",
    "lint-staged": "^6.1.0",
    "lodash.debounce": "^4.0.8",
    "lodash.sortby": "^4.7.0",
    "lodash.uniq": "^4.5.0",
    "marked": "^0.3.12",
    "marky-mark": "^1.2.2",
    "mocha": "^5.0.0",
    "node-dev": "^3.1.3",
    "prettier": "^1.10.2",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-ace": "^5.9.0",
    "react-bootstrap": "^0.32.0",
    "react-copy-to-clipboard": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-icons": "^2.2.7",
    "react-overlays": "^0.8.3",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-s-alert": "^1.4.1",
    "react-select": "^1.2.1",
    "react-split-pane": "^0.1.74",
    "sql-formatter": "^2.1.0",
    "supertest": "^3.0.0",
    "tachyons": "^4.9.1",
    "taucharts": "^1.2.1",
    "watch": "^1.0.2",
    "whatwg-fetch": "^2.0.1"
  }
}
